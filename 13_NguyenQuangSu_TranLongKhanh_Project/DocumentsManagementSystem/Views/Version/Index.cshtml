@{
}
@using DocumentsManagementSystem.Models
<form asp-action="restore" asp-controller="version" method="post">
<table>
    @foreach (DocumentsManagementSystem.Models.Version d in Model)
    {
        <tr style="display:flex;justify-content:space-between">
            <td>@d.VersionId</td>
            <td>@d.UpdatedContent</td>
            <td>@d.UpdatedTime</td>
            <input type="hidden" name="versionid" value="@d.VersionId"/>
            <input type="hidden" name="docid" value="@d.DocId"/>
            <td> <input type="submit" name="name" value="restore" /></td>
        </tr>
    }
</table>
</form>